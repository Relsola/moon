[
  {
    "label": "16 进制颜色值",
    "value": "^#([a-fA-F0-9]{6}|[a-fA-F0-9]{3})$",
    "example": "#ffbbad | #Fc01DF | #FFF | #ffE"
  },
  {
    "label": "24 小时制时间",
    "value": "^(0?d|1d|2[0-3]):(0?d|[1-5]d)$",
    "example": "23:59 | 02:07"
  },
  {
    "label": "window 操作系统文件路径",
    "value": "^[a-zA-Z]:\\\\(?:[^\\\\:*<>|\"?\\r\\n/]+\\\\)*[^\\\\:*<>|\"?\\r\\n/]*\\\\?$",
    "example": "F:\\learn\\regex\\ | F:\\"
  },
  {
    "label": "不匹配任何字符",
    "value": ".^",
    "example": "不匹配任何字符"
  },
  {
    "label": "HTML id 属性",
    "value": "id=\"[^\"]+\"",
    "example": "<div id=\"#app\"></div>"
  },
  {
    "label": "数字千位分隔符表示法",
    "value": "^\\B(?=(\\d{3})+\\b)$",
    "example": "12,345,678"
  },
  {
    "label": "浮点数(小数)",
    "value": "^[+-]?(\\d+\\.\\d+|\\d+|\\.\\d+)$",
    "example": "1.23 | +1.23 | -1.23"
  },
  {
    "label": "手机号码",
    "value": "^1[3-9]\\d{9}$",
    "example": "13123456789"
  },
  {
    "label": "邮箱",
    "value": "^[a-zA-Z0-9_-]+@[a-zA-Z0-9_-]+(\\.[a-zA-Z0-9_-]+)+$",
    "example": "13123456789"
  },
  {
    "label": "URL",
    "value": "^(http|https)://[a-zA-Z0-9]+(\\.[a-zA-Z0-9]+)+(/[a-zA-Z0-9]+)*$",
    "example": "https://www.baidu.com"
  },
  {
    "label": "IP 地址",
    "value": "^((25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]?\\d)\\.){3}(25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]?\\d)$",
    "example": "https://www.baidu.com"
  },
  {
    "label": "日期",
    "value": "^\\d{4}-(0\\d|1[0-2])-(0[1-9]|[12]\\d|3[01])$",
    "example": "2020-01-01"
  },
  {
    "label": "QQ 号",
    "value": "^[1-9]\\d{4,10}$",
    "example": "123456"
  },
  {
    "label": "微信号",
    "value": "^[a-zA-Z]([-_a-zA-Z0-9]{5,19})+$",
    "example": "wei_xin"
  },
  {
    "label": "车牌号",
    "value": "^[\\u4e00-\\u9fa5]{1}[a-zA-Z]{1}[a-zA-Z_0-9]{5}$",
    "example": "粤B12345"
  },
  {
    "label": "中文",
    "value": "^[\\u4e00-\\u9fa5]+$",
    "example": "中文"
  },
  {
    "label": "密码(包含大小写、数字、特殊字符且大于6位)",
    "value": "^(?=.*[A-Z])(?=.*[a-z])(?=.*\\d)(?=.*[!@#%&])^[A-Za-z\\d!@#%&]{6,}$",
    "example": "password"
  },
  {
    "label": "邮政编码",
    "value": "^[1-9]\\d{5}$",
    "example": "123456"
  },
  {
    "label": "身份证号",
    "value": "^\\d{17}[\\dXx]$",
    "example": "123456"
  }
]
